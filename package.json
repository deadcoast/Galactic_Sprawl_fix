{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "lint:critical": "eslint --cache --ext .ts,.tsx src/game-loop src/physics src/render-engine --max-warnings=0",
    "lint:changed": "eslint --cache $(git diff --name-only --diff-filter=ACMR | grep -E '\\.(js|jsx|ts|tsx)$')",
    "preview": "vite preview",
    "fix-imports": "node tools/fix-imports.js",
    "fix-imports-v2": "node tools/fix-imports-v2.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage",
    "test:jest:resource": "jest src/tests/managers/resource/",
    "coverage": "vitest run --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "type-check": "tsc --noEmit --downlevelIteration --skipLibCheck",
    "type-check:downlevel": "tsc --project tsconfig.check.json",
    "format:all": "prettier --write \"src/**/*.{ts,tsx,css,scss}\" && npm run lint:fix",
    "validate": "npm-run-all --parallel type-check lint:critical",
    "precommit": "lint-staged && npm run type-check",
    "find-circular": "npx madge --circular --extensions ts,tsx src/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "lint:track": "node tools/track-eslint-progress.js",
    "lint:chart": "node tools/eslint-progress-chart.js",
    "lint:fix-rule": "node tools/fix-eslint-by-rule.js",
    "lint:status": "node tools/eslint-progress-chart.js && echo '\\nTop issues:' && node tools/fix-eslint-by-rule.js --threshold=5",
    "lint:fix-top": "node tools/fix-eslint-by-rule.js --auto",
    "lint:fix-critical": "npm run lint:fix-rule -- no-unused-vars --auto && npm run lint:fix-rule -- prefer-const --auto && npm run lint:fix-rule -- eqeqeq --auto",
    "format:prettier": "node tools/fix-eslint-by-rule.js --prettier",
    "lint:workflow": "node tools/run-lint-workflow.js",
    "lint:workflow:interactive": "node tools/run-lint-workflow.js --interactive",
    "lint:workflow:auto-fix": "node tools/run-lint-workflow.js --auto-fix",
    "lint:workflow:analyze": "node tools/run-lint-workflow.js --skip-fix"
  },
  "dependencies": {
    "@pixi/particle-emitter": "^5.0.8",
    "@radix-ui/react-tabs": "^1.1.3",
    "@react-spring/three": "^9.7.5",
    "@react-three/drei": "^9.121.4",
    "@react-three/fiber": "^8.17.14",
    "@types/d3": "^7.4.3",
    "@types/serve-static": "^1.15.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "d3": "^7.9.0",
    "framer-motion": "^12.4.2",
    "lucide-react": "^0.344.0",
    "pixi-spine": "^4.0.4",
    "pixi.js": "^7.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.2.0",
    "rxjs": "^7.8.1",
    "serve-static": "^1.16.2",
    "tailwind-merge": "^2.1.0",
    "three": "^0.173.0",
    "three-mesh-bvh": "^0.8.0",
    "uuid": "^11.0.5",
    "xstate": "^5.19.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.26.8",
    "@eslint/js": "^9.21.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.50.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.173.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "glob": "^10.3.10",
    "globals": "^15.9.0",
    "gsap": "^3.12.7",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.0.0",
    "madge": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "prettier": "^3.5.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^5.0.5",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.0",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "main": "eslint.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deadcoast/Galactic_Sprawl_fix.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deadcoast/Galactic_Sprawl_fix/issues"
  },
  "homepage": "https://github.com/deadcoast/Galactic_Sprawl_fix#readme",
  "description": "",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}

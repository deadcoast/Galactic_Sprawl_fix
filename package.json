{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "fix-imports": "node tools/fix-imports.js",
    "fix-imports-v2": "node tools/fix-imports-v2.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "format:all": "prettier --write \"src/**/*.{ts,tsx,css,scss}\" && npm run lint:fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@pixi/particle-emitter": "^5.0.8",
    "@react-spring/three": "^9.7.5",
    "@react-three/drei": "^9.121.4",
    "@react-three/fiber": "^8.17.14",
    "@types/serve-static": "^1.15.7",
    "framer-motion": "^12.4.2",
    "lucide-react": "^0.344.0",
    "pixi-spine": "^4.0.4",
    "pixi.js": "^7.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxjs": "^7.8.1",
    "serve-static": "^1.16.2",
    "three": "^0.173.0",
    "three-mesh-bvh": "^0.8.0",
    "uuid": "^11.0.5",
    "xstate": "^5.19.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.26.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.173.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "glob": "^10.3.10",
    "globals": "^15.9.0",
    "gsap": "^3.12.7",
    "husky": "^9.0.0",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.5.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rimraf": "^5.0.5",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.24.0",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "main": "eslint.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deadcoast/Galactic_Sprawl_fix.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deadcoast/Galactic_Sprawl_fix/issues"
  },
  "homepage": "https://github.com/deadcoast/Galactic_Sprawl_fix#readme",
  "description": "",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}

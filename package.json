{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "lint:critical": "eslint --cache --ext .ts,.tsx src/game-loop src/physics src/render-engine --max-warnings=0",
    "lint:changed": "eslint --cache $(git diff --name-only --diff-filter=ACMR | grep -E '\\.(js|jsx|ts|tsx)$')",
    "preview": "vite preview",
    "fix-imports": "node tools/fix-imports.js",
    "fix-imports-v2": "node tools/fix-imports-v2.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest src/tests/components src/tests/utils src/tests/hooks src/tests/managers",
    "test:integration": "vitest src/tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:perf": "vitest src/tests/performance",
    "test:tools": "vitest src/tests/tools",
    "coverage": "vitest run --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "type-check": "tsc --noEmit --downlevelIteration --skipLibCheck",
    "type-check:downlevel": "tsc --project tsconfig.check.json",
    "format:all": "prettier --write \"src/**/*.{ts,tsx,css,scss}\" && npm run lint:fix",
    "validate": "npm-run-all --parallel type-check lint:critical",
    "precommit": "lint-staged && npm run type-check",
    "find-circular": "npx madge --circular --extensions ts,tsx src/",
    "lint:track": "node tools/track-eslint-progress.js",
    "lint:chart": "node tools/eslint-progress-chart.js",
    "lint:fix-rule": "node tools/fix-eslint-by-rule.js",
    "lint:status": "node tools/eslint-progress-chart.js && echo '\\nTop issues:' && node tools/fix-eslint-by-rule.js --threshold=5",
    "lint:fix-top": "node tools/fix-eslint-by-rule.js --auto",
    "lint:fix-critical": "npm run lint:fix-rule -- no-unused-vars --auto && npm run lint:fix-rule -- prefer-const --auto && npm run lint:fix-rule -- eqeqeq --auto",
    "format:prettier": "node tools/fix-eslint-by-rule.js --prettier",
    "lint:workflow": "node tools/run-lint-workflow.mjs",
    "lint:workflow:interactive": "node tools/run-lint-workflow.mjs --interactive",
    "lint:workflow:auto-fix": "node tools/run-lint-workflow.mjs --auto-fix",
    "lint:workflow:analyze": "node tools/run-lint-workflow.mjs --skip-fix",
    "typecheck": "tsc --noEmit --downlevelIteration --skipLibCheck",
    "typecheck:specific": "node tools/run-typecheck.mjs"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@chakra-ui/react": "^3.13.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "@radix-ui/react-tabs": "^1.1.3",
    "@react-spring/three": "^9.7.5",
    "@react-three/drei": "^9.121.4",
    "@react-three/fiber": "^8.17.14",
    "@types/d3": "^7.4.3",
    "@types/serve-static": "^1.15.7",
    "antd": "^5.24.3",
    "chart.js": "^4.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "d3": "^7.9.0",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "eventemitter3": "^5.0.1",
    "framer-motion": "^12.4.2",
    "geojson": "^0.5.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.344.0",
    "pixi.js": "^7.4.2",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.2.0",
    "react-virtualized-auto-sizer": "^1.0.25",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^2.15.1",
    "reselect": "^5.1.1",
    "rxjs": "^7.8.1",
    "serve-static": "^1.16.2",
    "tailwind-merge": "^2.1.0",
    "three": "^0.173.0",
    "typescript-eslint": "^8.27.0",
    "uuid": "^11.0.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@babel/traverse": "^7.26.10",
    "@eslint/js": "^9.21.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.50.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-window": "^1.8.8",
    "@types/react-window-infinite-loader": "^1.0.9",
    "@types/three": "^0.173.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.21",
    "commander": "^13.1.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.0.0",
    "madge": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.15",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^5.4.2",
    "vitest": "^1.3.1",
    "ws": "^8.18.1"
  },
  "main": "eslint.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deadcoast/Galactic_Sprawl_fix.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deadcoast/Galactic_Sprawl_fix/issues"
  },
  "homepage": "https://github.com/deadcoast/Galactic_Sprawl_fix#readme",
  "description": "## Overview",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "type": "module"
}
